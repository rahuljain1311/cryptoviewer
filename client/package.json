{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "lint-staged": {
    "gitDir": "../",
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "clipboard": "^1.7.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "history": "^4.7.2",
    "jest-cli": "20.0.4",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "prettier": "^1.6.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-datetime": "^2.10.1",
    "react-dom": "^15.6.1",
    "react-google-maps": "^8.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-page-transition": "^3.0.2",
    "react-router-redux": "next",
    "react-router-transition": "^1.0.0",
    "react-scripts": "1.0.13",
    "react-slick": "^0.15.4",
    "react-test-renderer": "15.6.2",
    "recompose": "^0.25.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "slick-carousel": "^1.8.1"
  },
  "scripts": {
    "start": "../purescriptBuild.sh && REACT_APP_BUILD_TYPE=dev react-scripts start",
    "build": "../purescriptBuild.sh && REACT_APP_BUILD_TYPE=production react-scripts build",
    "build-stage": "../purescriptBuild.sh && REACT_APP_BUILD_TYPE=staging react-scripts build",
    "build-dev": "../purescriptBuild.sh && REACT_APP_BUILD_TYPE=dev react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint ./src --ext .js,.jsx"
  },
  "jest": {
    "collectCoverageFrom": [
      "!src/containers/*",
      "!src/tests/*",
      "src/**/*.{jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "text"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.3.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.1.0"
  }
}
